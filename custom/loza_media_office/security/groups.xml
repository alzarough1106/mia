<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record model="ir.module.category" id="module_category_media_campaign">
            <field name="name">Media Groups</field>
            <field name="description">Helps you handle your Media Office work Access Rights</field>
            <field name="visible" eval="0"/>
            <field name="sequence">1</field>
        </record>

        <record id="group_media_admin" model="res.groups">
            <field name="name">Media Office Admin</field>
            <field name="comment">A Media Office Admin user group</field>
            <field name="category_id" ref="module_category_media_campaign"/>
            <field name="users"
                   eval="[(4, ref('base.user_admin'))]"/>
        </record>
        <record id="group_media_member" model="res.groups">
            <field name="name">Media Office Member</field>
            <field name="comment">A Media Office Member user group</field>
            <field name="category_id" ref="module_category_media_campaign"/>
<!--            <field name="implied_ids" eval="[(4, ref('account.group_account_invoice')), (4, ref('group_lab'))]"/>-->
            <field name="users"
                   eval="[(4, ref('base.user_admin'))]"/>
        </record>
        <record id="group_media_task_owner" model="res.groups">
            <field name="name">Media Designated Event Office</field>
            <field name="comment">A Media Designated Event Office group</field>
            <field name="category_id" ref="module_category_media_campaign"/>
<!--            <field name="implied_ids" eval="[(4, ref('account.group_account_invoice')), (4, ref('group_lab'))]"/>-->
            <field name="users"
                   eval="[(4, ref('base.user_admin'))]"/>
        </record>

    </data>
</odoo>