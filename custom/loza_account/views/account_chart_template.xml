<?xml version="1.0" encoding="UTF-8"?>
<odoo>
        <record id="account_chart_template_inherit_form_loza_account" model="ir.ui.view">
            <field name="name">account.chart.template.inherit.form.loza.account</field>
            <field name="model">account.chart.template</field>
            <field name="inherit_id" ref="account.view_account_chart_template_form"/>
            <field name="arch" type="xml">
                <field name="complete_tax_set" position="after">
                    <field name="account_journal_suspense_account_id"/>
                    <field name="default_cash_difference_expense_account_id"/>
                    <field name="default_cash_difference_income_account_id"/>
                    <field name="default_pos_receivable_account_id"/>
                    <field name="currency_id"/>
                    <field name="expense_currency_exchange_account_id"/>
                    <field name="income_currency_exchange_account_id"/>
                    <field name="property_account_expense_categ_id"/>
                    <field name="property_account_expense_id"/>
                    <field name="property_account_income_categ_id"/>
                    <field name="property_account_income_id"/>
                    <field name="property_account_payable_id"/>
                    <field name="property_account_receivable_id"/>
                    <field name="property_advance_tax_payment_account_id"/>
                    <field name="property_cash_basis_base_account_id"/>
                    <field name="property_stock_account_input_categ_id"/>
                    <field name="use_anglo_saxon"/>
                    <field name="property_tax_receivable_account_id"/>
                    <field name="property_tax_payable_account_id"/>
                    <field name="property_stock_valuation_account_id"/>
                    <field name="property_stock_account_output_categ_id"/>
                </field>
                <field name="tax_template_ids" position="after">
                    <field name="account_ids"/>
                </field>
            </field>
        </record>
</odoo>

